<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
          "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="default" extends="struts-default" namespace="/">
    	<action name="listUI" class="action.UserAction" method="listUI">
    		<result name="success">/listUI.jsp</result>
    	</action>

    	<action name="editUI" class="action.UserAction" method="editUI">
    		<result name="success">/editUI.jsp</result>
    	</action>
    	<action name="edit" class="action.UserAction" method="edit">
    		<result name="success">/listUI.jsp</result>
    	</action>

    	<action name="delete" class="action.UserAction" method="delete">
    		<result name="success">/listUI.jsp</result>
    	</action>
    	<action name="addUser" class="action.UserAction" method="addUser">
    		<result name="success">/listUI.jsp</result>
    	</action>
    </package> 
</struts>
